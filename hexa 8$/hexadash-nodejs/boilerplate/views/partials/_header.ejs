<!doctype html>
<html lang="en" dir="<%- locals.dir %>">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>HexaDash - <%- title %></title>
    
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@400;500;600;700&display=swap" rel="stylesheet">
    <% if (locals.dir === 'rtl') { %>
    <link rel="stylesheet" href="<%- locals.base %>css/plugin.min.rtl.css">
    <link rel="stylesheet" href="<%- locals.base %>css/style.rtl.css">
    <% } else { %>
        <link id="pluginStyleSheet" rel="stylesheet" href="<%- locals.base %>css/plugin.min.css">
        <link id="customStyleSheet" rel="stylesheet" href="<%- locals.base %>css/style.css">
    <% } %>
    <link rel="icon" type="image/png" sizes="16x16" href="<%- locals.base %>img/favicon.svg">
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v3.0.0/css/line.css">
</head>